# ============================================================================
# Environment Configuration - RUL Prediction System
# ============================================================================

# Application
APP_ENV=production
APP_DEBUG=false
LOG_LEVEL=info
VERSION=latest

# Database - PostgreSQL (using alternative port)
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=rul_prediction
POSTGRES_PORT=15432

# Redis (using alternative port)
REDIS_PORT=16379

# API
API_PORT=18000

# Dashboard
DASHBOARD_PORT=13000

# Airflow
AIRFLOW_WEBSERVER_PORT=18080
AIRFLOW_FLOWER_PORT=15555

# Monitoring
PROMETHEUS_PORT=19090
GRAFANA_PORT=13001

# Model
MODEL_VERSION=1.0.0

# Security (Use secret manager for production)
SECRET_KEY=your-secret-key-change-in-production
API_KEY=your-api-key

# Airflow Configuration
AIRFLOW_UID=50000
AIRFLOW__CORE__EXECUTOR=CeleryExecutor
AIRFLOW__CORE__LOAD_EXAMPLES=False
AIRFLOW__CORE__DAGS_ARE_PAUSED_AT_CREATION=True
AIRFLOW__CORE__LOAD_DEFAULT_CONNECTIONS=False
